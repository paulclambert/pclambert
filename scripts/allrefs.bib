@Article{Lambert2011,
  author      = {P. C. Lambert and L. Holmberg and F. Sandin and F. Bray and K. M. Linklater and A. Purushotham and D. Robinson and H. M{\o}ller},
  title       = {Quantifying differences in breast cancer survival between {E}ngland and {N}orway.},
