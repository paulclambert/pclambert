  keywords        = {Adult; Aged; Aged, 80 and over; Fatigue, diagnosis, etiology, psychology; Female; Humans; Leukemia, Myelogenous, Chronic, BCR-ABL Positive, complications, drug therapy, psychology; Longitudinal Studies; Male; Middle Aged; Prospective Studies; Protein Kinase Inhibitors, administration & dosage, adverse effects; Pyrimidines, adverse effects; Quality of Life; Self Report, statistics & numerical data; Severity of Illness Index; Treatment Outcome; Young Adult; chronic myeloid leukemia; fatigue; nilotinib; quality of life; tyrosine kinase inhibitors},
  month           = may,
  nlm-id          = {0374236},
  owner           = {NLM},
  pmid            = {29499087},
  pubmodel        = {Print-Electronic},
