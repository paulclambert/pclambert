@Article{Fink2012,
  author      = {Fink, Aliza K. and German, Robert R. and Heron, Melonie and Stewart, Sherri L. and Johnson, Christopher J. and Finch, Jack L. and Yin, Daixin and Schaeffer, Philip E. and , Accuracy of Cancer Mortality Working Group},
