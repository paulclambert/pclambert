  issn-linking    = {1380-7870},
  keywords        = {Biostatistics; Causality; Humans; Likelihood Functions; Proportional Hazards Models; Randomized Controlled Trials as Topic, statistics & numerical data; Survival Analysis; Treatment Outcome; Causation; Collapsible model; Confounding; Hazard function; Survival data},
