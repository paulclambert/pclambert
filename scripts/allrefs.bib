  issn-linking    = {0284-1851},
  keywords        = {Adult; Age Factors; Aged; Body Mass Index; Breast, diagnostic imaging; Breast Density; Breast Neoplasms, diagnostic imaging, pathology; Female; Humans; Magnetic Resonance Imaging, methods; Middle Aged; Risk Factors; Magnetic resonance imaging; breast; epidemiology; primary neoplasms},
