  issn-linking    = {0959-8049},
  keywords        = {Data Collection; Humans; Male; Prostatic Neoplasms, diagnosis, epidemiology, pathology; Registries; Sweden; Cancer quality register; Evaluation; Prostate cancer; Validity},
  nlm-id          = {9005373},
  owner           = {NLM},
