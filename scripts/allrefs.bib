@Article{Mo2014,
  author          = {Mo, X.-D. and Jiang, Q. and Xu, L.-P. and Liu, D.-H. and Liu, K.-Y. and Jiang, B. and Jiang, H. and Chen, H. and Chen, Y.-H. and Zhang, X.-H. and Han, W. and Wang, Y. and Huang, X.-J.},
  title           = {Health-related quality of life of patients with newly diagnosed chronic myeloid leukemia treated with allogeneic hematopoietic SCT versus imatinib.},
