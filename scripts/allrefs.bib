@Article{Ito2014,
  author          = {Ito, Yuri and Nakaya, Tomoki and Nakayama, Tomio and Miyashiro, Isao and Ioka, Akiko and Tsukuma, Hideaki and Rachet, Bernard},
  title           = {Socioeconomic inequalities in cancer survival: a population-based study of adult patients diagnosed in {O}saka, {J}apan, during the period 1993-2004.},
