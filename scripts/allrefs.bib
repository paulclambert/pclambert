  issn-linking    = {1556-0864},
  keywords        = {Female; Humans; Incidence; Kaplan-Meier Estimate; Lung Neoplasms, epidemiology, mortality; Male; Poverty, statistics & numerical data; Rural Population, statistics & numerical data; Socioeconomic Factors; United Kingdom; Urban Population, statistics & numerical data},
  nlm-id          = {101274235},
