  keywords        = {Aged; Biomarkers, blood; Blood Glucose, metabolism; Diabetes Mellitus, Type 2, blood, epidemiology; Follow-Up Studies; Glucose Clamp Technique; Glucose Tolerance Test; Humans; Incidence; Insulin Resistance; Longitudinal Studies; Male; Reference Values; Selenium, blood; Sweden, epidemiology},
  nlm-id          = {8500858},
