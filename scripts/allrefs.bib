@Article{Jabbour2020,
  author          = {Jabbour, Elias and Kantarjian, Hagop},
  title           = {Chronic myeloid leukemia: 2020 update on diagnosis, therapy and monitoring.},
  doi             = {10.1002/ajh.25792},
  issn            = {1096-8652},
