  issn-linking    = {1877-7821},
  keywords        = {Early Detection of Cancer, methods; Electronic Health Records; England, epidemiology; Humans; Meta-Analysis as Topic; Mortality, trends; Neoplasms, diagnosis, epidemiology, mortality; Population Surveillance; Prognosis; Cancer mortality; Cancer survival; Diagnostic interval; Early detection; Early diagnosis; Emergency presentation; Population-based; Routine data; Stage at diagnosis; Surveillance},
  nlm-id          = {101508793},
  owner           = {NLM},
  pii             = {S1877-7821(18)30368-0},
  pmc             = {PMC6189520},
  pmid            = {30056051},
